// Bring in all Compass Helpers 
// as well as Tito's base style files 
@import "partials/base";


/*	 Author: Mike Swartz			 */ 


a {
	text-decoration: none;	color: $link;
	&:hover {color: darken($link, 10%);}
	&:visited {	color: $link; }
}
	
strong, .strong { font-weight: bold; }

em { font-style: italic; }

.mainbar { 
	h1, h2, h3, h4, h5, h6 {
		font-family: $serif;
		font-weight: normal;
	}
}


/* ============================================= */

/*	UNIVERSAL							   	     */ 

/* ============================================= */


body {
	background-color: $beat-taupe;
	background-image: url(/wp-content/themes/_design/_img/template/top-pattern.png), url(/wp-content/themes/_design/_img/template/paper.png);
	background-repeat: repeat-x, repeat;
	background-position: top left, top left;
	background-attachment: fixed, fixed;
	padding: 10px;
	
	@media screen and (max-width:768px) {
		padding: 5px;
	}
}

.wrapper {
	max-width: 1200px;
	margin: 0 auto;
	
	@media screen and (max-width:959px) {
		padding: 0 30px;
	}
}

#content {
	@include ui-grid-lr(70%, 30%, 0);
	
	/* Kill sidebar on iPad */
	@media screen and (max-width:768px) {
		.mainbar {
			&.ui-block-1 { 
				width: 100%;
			}
		}
		
		.sidebar {			
			&.ui-block-2 { 
				width: 100%;
			}
		}
	}
	
	@media screen and (max-width:480px) {
	
		.ui-block-1, .ui-block-2 { 
			width: 100%;
		}
	}
	
	
}


.mainbar { 
	background-color: white;
	@include border-radius(0 0 5px 5px);
	font-family: $serif;
	
	.entry-title {
	}
	
	.entry-title a { 
		font-size: 22px;
		margin: 0 0 0px 0;
		color: $text-black;
		&:hover {
			color: $link;
		}
		
		@media screen and (max-width:768px) {
			font-size: 2em;
		}
	}
	
	h1 {
		font-size: 3em;
		margin: 0 0 0px 0;
		padding: 20px 20px 5px 20px;
		color: $txt-black;
		
		@media screen and (max-width:768px) {
			font-size: 2em;
		}
		
	}

	
	.entry-utility {
		background-color: $beat-taupe;
		margin: 0 0 0px 0;
		border-top: 2px dashed white;
		border-bottom: 1px dotted $beat-taupe-dark;
		font-family: $sans;
		font-size: 12px;
		padding: 10px;
//		box-shadow: inset 0px -2px 5px $beat-taupe-dark;
		
		a { 
		display: inline-block;
		color: $blue;
		margin: 0 10px 0 0;
		@include trans;
			&:hover {
				color: darken($blue, 10%);
			}
		}
	}
	
	.entry-meta {
		padding: 0 20px;
		font-family: $sans;
		font-size: 12px;
		a {
			color: $link;
		}
	 }
	
	.posted-on {
		margin: 0 0 10px 0;
		font-family: $sans;
		font-size: 12px;
		a {
			color: $link;
		}
	}
	
	.entry-wrapper {
		@include ui-grid-lr(30%, 68%, 2%);
		padding: 0 20px;
		.post-thumb img { width: 100%; height: auto; }
	}
	
	.post {
		margin: 0 0 20px 0;
		padding: 0px 0 0 0;
	}
	
	p {
		font-size: 16px;
		line-height: 24px;
		margin: 0 0 20px 0;
		
		img.size-large {
			width: 100%;
			height: auto;
		}
	}
	
}


/* ============================================= */

/*	HOMEPAGE     						   	     */ 

/* ============================================= */


#cover-img-container {
	max-height: 450px;
	overflow: hidden;
	margin-bottom: 50px;
	img {
		width: 100%;
		height: auto;
	}
}

#cover-text {
	margin: -220px 20px 20px 20px;
	height: 180px;
	background: rgba(255,255,255,.75);
	padding: 10px;
	z-index: 1000;
	position: relative;
	
	box-shadow: inset 0 0 100px #FFF;
}

.cover-entry-title {
	font-size: 28px;
	a {
		color: $text-black;
	}
}


/* ============================================= */

/*	SIDEBAR      						   	     */ 

/* ============================================= */


.sidebar {
	margin-top: 0px;
	padding: 20px 0 0 35px;
	
	@media screen and (max-width:768px) {
		margin: 0;
	}
	
	
	#twitter-2 {
		h3 a {
			font-size: 20px;
			color: $link;
		}
		.tweets {
			padding: 10px;
		
			li {
				padding-bottom: 10px;
				margin-bottom: 10px;
				border-bottom: 1px dotted #CCC;
				
				.timesince {
					color: $black-50;
				}
				a {
					padding: 0;
					font-size: inherit;
				}
			}
		}
	}
	
	#social {
		width: 100%;
		text-align: center;
		margin: 0 0 20px 0;
		@include background-image(linear-gradient(top, #FAFAF7, $beat-side));
		@include border-radius(6px);
		
		a {
			width: 32%;
			display: block;
			@include inline-block;
			height: 45px;
			padding: 10px;
			
			
			background-repeat: no-repeat;
			background-position: center;
			text-indent: -10000px;
			
			@include trans;
			
			&:hover {
				background-color: $black-15;
				background-position: center 28%;
			}
			
			&.twitter {
				background-image: url(/wp-content/themes/_design/_img/template/twitter_32.png);
			}
			
			&.facebook {
				background-image: url(/wp-content/themes/_design/_img/template/facebook_32.png);
			}
			
			&.flickr {
				background-image: url(/wp-content/themes/_design/_img/template/flickr_32.png);
			}
		}
		
	}
	
	
	h3, h3 a {
		font-size: 16px;
		font-family: $serif;
		color: #FFF;
		font-weight: normal;
		background-color: $beat-blue;
		padding: 8px;
		
		@media screen and (max-width:768px){
			font-size: 1.5em;
			font-family: $sans;
		}
	}
	
	.widget-container {
		margin: 0 0 20px 0;
		background-color: #FAFAF7;
		@include border-radius(6px);
		
		div {
			padding: 10px;
		}
		
		img {
			width: 100%;
			height: auto;
		}
	}
	
	.widget_archive {
		ul { padding: 10px; }
	}
	
	.widget_search {
		@include pie-clearfix;
		padding: 10px;
		
		div {
			padding: 0;
		}
		
		label {
			display: none;
		}
		
		#s {
			padding: 8px 2%;
			width: 71%;
			float: left;
			border-left: 1px solid $black-15;
			border-top: 1px solid $black-15;
			border-bottom: 1px solid $black-15;
			
			&:focus {
				outline: 0;
			}
		}
		
		#searchsubmit {
			padding: 8px 2%;
			width: 21%;
			margin: 0 0 0 -1px;
			float: left;
			@include btn-light;
			border: 1px solid $black-15;
			@include border-radius(0 8px 8px 0);
		}
		
	}/* #search */
	
	.execphpwidget {
		font-family: $serif;
		font-size: 18px;
		
		ul li {
			border-bottom: 1px dotted $beat-side;
			padding: 5px 0;
			
			&:last-child {
				border: 0;
			}
			a {
				font-family: $serif;
				font-size: 18px;
			}
		}
		
		img {
			width: 100%;
			height: auto;
		}
		
	}
	
	#menu-neighborhoods {
		@include column-count(2);
	}
	
	ul li {
		margin: 0 0 5px 0;
		a {
			padding: 4px;
			//font-size: 14px;
			@include border-radius(4px);
			
			&:hover {
				background-color: $black-10;
			}
		}
	}
}

#side-more-sticky-wrapper {
	width: 100%;
}

#searchform {
	@include pie-clearfix;
	
	label {
		display: none;
	}
	
	#s {
		padding: 8px 2%;
		width: 71%;
		float: left;
		border-left: 1px solid $black-15;
		border-top: 1px solid $black-15;
		border-bottom: 1px solid $black-15;
		
		&:focus {
			outline: 0;
		}
	}
	
	#searchsubmit {
		padding: 8px 2%;
		width: 21%;
		margin: 0 0 0 -1px;
		float: left;
		@include btn-light;
		border: 1px solid $black-15;
		@include border-radius(0 8px 8px 0);
	}
		
}

/* ============================================= */

/*	SOCIAL STUFF						   	     */ 

/* ============================================= */


.fb-like {
	@include inline-block;
}


/* ============================================= */

/*	SINGLE POSTS						   	     */ 

/* ============================================= */

.single, .page {
	.post-thumb img { 
		width: 100%; 
		height: auto; 
		margin: 0 0 20px 0;
	}

	.comment p { padding: 0 10px 0 10px;	}

	#nav-above, #nav-below { display: none;	}

	.entry-utility { padding: 10px;	}

	.wp-caption {
		background: none;
		border: none;

		.wp-caption-text {
			font-size: 14px;
			text-align: left;
		}
	}
	
	p {		max-width: 600px;	}
	
	.entry-meta {	
		color: $beat-dark; @include pie-clearfix; @include ui-grid-lr(20%, 80%);

	}
	
	.share-tools {
		margin: 0 0 0 10px;
	}

	.share-fb { @include inline-block; }
	
	
	.entry-content {
		padding: 20px;
		
		&.single {
			margin: 0 auto;
			max-width: 680px;
			
			p img.size-large {
				width: 100%;
				height: auto;
			}
		}
		
		ul {
			list-style-type: disc;
			list-style-position: inside;
			font-size: 16px;
			margin-bottom: 20px;
			
			li { margin-bottom: 10px; line-height: 24px; }
		}
		
		ol {
			list-style-type: decimal;
			list-style-position: inside;
			font-size: 16px;
			margin-bottom: 20px;
			
			li { margin-bottom: 10px; line-height: 24px; }
		}
		
		a {
			color: $link;
			
			&:hover {	color: darken($link, 10%);	}
		}
		
		blockquote {
			padding: 0 0 0 20px;
			font-style: italic;
			border-left: 1px dotted $beat-text;
		}
		
		h1 { font-size: 2.2em; padding: 0; }
		h2 { font-size: 2em; }
		h3 { font-size: 1.8em; }
		h4 { font-size: 1.5em; }
		
		@media screen and (max-width:480px) {
						
			img {
				width: 100%;
				height: auto;
				
				&.alignright, &.alignleft {
					margin: 0;
				}
			}
		}
		
	} /* .entry-content */
	
	#entry-author-info {
		padding: 20px;
		border-top: 1px dotted $beat-text;
		@include pie-clearfix;
		font-family: $sans;
		
		h2 {
			font-family: $sans;
			font-weight: bold;
		}
		
		img {
			float: left;
			margin: 0 20px 0px 0;
		}
	}
	
	
	.post-related {
		margin-top: 20px;
		h3 {
			font-size: 20px;
			margin-bottom: 20px;
		}
		
		ul {
			list-style-type: none;
		}
		li {
			padding-bottom: 10px;
			margin-bottom: 10px;
			border-bottom: 1px dotted #CCC;
			
			&:last-child {
				border: 0;
				padding-bottom: 0px;
				margin-bottom: 0px;
			}
			a {
			font-size: 1.2em;
			}
		}
	}

} /* single */


#nav-below, #nav-above {
	background-color: $beat-taupe-dark;
	padding: 20px 10px;
	@include pie-clearfix;
	@include border-radius(0 0 5px 5px);
	
	.nav-previous {
		float: left;

	}
	
	.nav-next {
		float: right;

	}
	
	a {
		@include border-radius(8px);
		padding: 10px;
		background-color: rgba(0,0,0,.2);
		@include trans;
		color: white;
		
		&:hover {
			background-color: rgba(0,0,0,.25);
		}
	}
}/* nav above below */

#nav-above {
	@include border-radius(0 0 0px 0px);
}

.home {
	#nav-above {
		display: none;
	}
	
	.entry-content p {
		font-size: 15px;
		line-height: 22px;
	}
}


/* ============================================= */

/*	HEADER          					   	     */ 

/* ============================================= */



header {
	padding: 20px 0 0 0;
	@include pie-clearfix;
	
	h1 {
		cursor: pointer;
		width: 100%;
		display: none;
		a {
			@include trans;
			display: block;
			height: 49px;
			text-indent: -10000px;
			width: 100%;
			&:hover {
				cursor: pointer;
				@include rotate(-2deg);
			}
		}
	}

	#logo {	
		width: 100%;
		display: block;
		text-align: center;
		
		img {
			width: 100%;
			max-width: 650px;
			height: auto;
		}
		
		&:hover {
			img {
				opacity: .8;
			}
		}
	}
	
	p {
		width: 28%;
		float: right;
		margin: 20px 0 0 0;
		color: $beat-text;
		font-size: 16px;
		font-family: $serif;
		font-style: italic;
	}
	
	
	@media screen and (max-width:767px) {
		height: 70px;
		
		#logo {
			width: 100%;
		}
		
		p {
			display: none;
		}
	}
	
	
	@media screen and (max-width:767px) {
		padding: 0;
		height: 70px;
		
		#logo {
			width: 100%;
			text-align: center;
			
			img {
				width: 80%;
				margin: 0 auto;
			}
		}
		
		p { display: none;	}
	}
} /* header */


/* ============================================= */

/*	 MAIN NAV			        		   	     */ 

/* ============================================= */

#access {
	width: 100%;
	background-color: $beat-blue;
	height: 40px;

	
	.menu-header {
		width: 70%;
		padding: 8px 0 0 0;
		@include pie-clearfix;
		@include inline-block;
		
		ul li {
			@include inline-block;
			
			&.current-menu-item {
				a {
					background-color: darken($beat-blue, 10%);
				}
			}
			
			a {
				@include trans;
				font-size: 18px;
				font-family: $serif;
				color: white;
				padding: 10px 8px;
				height: 45px;
				
				&:hover {
					background-color: darken($beat-blue, 10%);
				}
				
			} /* a */
		}/* ul li */
	} /* .menu-header */

	#searchform {
		width: 20%;
		@include inline-block;
	}
	
	

	
	@media screen and (max-width:768px) {
		width: 100%;
		
		.menu-header ul li a {
			padding: 10px 5px;
		}
	}
	
	@media screen and (max-width:480px) {
		width: 100%;
		@include border-radius(0);
		
		.menu-header {
			height: auto;
			@include border-radius(0px);
			display: none;
			
			ul li {
				text-align: center;
				width: 100%;
				border-bottom: 1px solid darken($beat-blue, 5%);
				padding: 0px 0;
				
				&:first-child a {
					@include border-radius(0px);	
				}
				
				a {
					display: block;
				}
			}
		}
	}

} /* #access */


/* ============================================= */

/*	COMMENTS        					   	     */ 

/* ============================================= */


#comments-title {
	padding: 10px;
	font-size: 20px;
	border-bottom: 1px dashed $beat-taupe-dark;
}

#reply-title {
	padding: 10px;
	font-size: 20px;
}

.mainbar .pingback {
	padding: 5px 10px;
	opacity: .75;
	border-bottom: 1px dashed $beat-taupe-dark;
	font-family: $sans;
	
	&:hover {
		opacity: 1;
	}
	
	p { margin: 0; font-size: 12px;}
}

.comments-closed {
	display: none;
}

.comment {
	padding: 10px;
	border-bottom: 1px dashed $beat-taupe-dark;
	position: relative;
	
	@include pie-clearfix;
	
	.ui-group {
		@include ui-grid-lr(5%,95%,0);
	}
	
	.comment-meta {
		color: $black-50;
		font-family: $sans;
		font-size: 11px;
		padding: 0 0 0 10px;
		
	}
	
	.reply {
	position: absolute;
	top: 20px;
	right: 0;
	
		a { 
			@include trans;
			opacity: 0;
			@include border-radius(5px 0 0 5px);
			background-color: $beat-blue;
			padding: 8px;
			margin: -8px 0 0 0;
			color: white;
			
			&:hover {
				background-color: darken($beat-blue, 10%);
			}
		}
	}/*reply*/
	
	&:hover .reply a {
		opacity: 1;
	}
}/*comment */

.children {
	background-color: $beat-taupe;
	@include border-radius(10px);
	margin: 20px 0 0 50px;
	.comment {
		
			border-bottom: 1px dotted $beat-taupe-dark;
			
			&:last-child{
			border: 0px;	
			}
	}
}

#respond {
	background-color: $beat-taupe;
	padding: 20px;
	
	.logged-in-as {
	margin: 0 0 10px 0;	
	}
	
	input {
		padding: 10px;
		border: 1px solid darken($beat-taupe-dark, 10%);
		width: 100%;
	}
	
	input[type=submit]{
		max-width: 150px;
		margin: 0 auto;
		@include border-radius (8px);
		border: 1px solid $black-15;
		@include btn-light;
	}
	
	.comment-form-comment {

		label {
			display: block;
		}
		
		
		textarea {
			width: 100%;
			border: 1px solid darken($beat-taupe-dark, 10%);
		}
	}
	
	.form-allowed-tags {
		font-size: 14px;
		color: darken($beat-text, 10%);
	}
	
	#cancel-comment-reply-link {
		color: $beat-red;
		float: right;
	}
	
	.form-submit input {
		background-color: darken($beat-taupe-dark, 10%);
	}
}/* respond */

#commentform {
	padding: 20px;
}

ol {
	list-style-type: none;
}


/* ============================================= */

/*	PAGE TEMPLATES  					   	     */ 

/* ============================================= */

#page-descrip {
	background-color: $beat-taupe;
	border-bottom: 2px dotted white;
	
	.entry-title, h1 { padding: 0;}
	h2 { font-size: 1em; }
	.entry-content, .about { padding: 20px;	}
	p {	color: $beat-text; }
	img { float: left; margin: 0 20px 0 0;}
}

.page .entry-content {
	padding: 20px;
	@include pie-clearfix;
	
	h1 { font-size: 2.2em; padding: 0; }
	h2 { font-size: 2em; }
	h3 { font-size: 1.8em; }
	h4 { font-size: 1.5em; }
	
}

.page #comments-title {
	background-color: $beat-taupe;
	padding-top: 10px;
}

.page .comment-body p, .page #respond p {
	padding-left: 10px;
}


/* SEARCH PAGES */

.search-again {
	padding: 20px;
}


/* TAG / CAT / ARCH PAGES */

.tag, .category, .archive {
	#page-descrip {
		padding: 10px;
	}
	
	.entry-content {
		padding: 10px;
	}
	
	.entry-title a.url {
		font-size: 1em;
	}
}

/* GALLERY */
.gallery {
	@include pie-clearfix;
	
	.gallery-item {
		float: left;
		margin: 0 10px 10px 0;
	}

}


.attachment {
	.page-title {
		padding: 10px;
	}
	.entry-title {
		font-size: 1.5em;
	}
	.entry-attachment {
		p img {
			max-width: 800px !important;
		}
	}
}


/* ============================================= */

/*	FOOTER          					   	     */ 

/* ============================================= */

footer[role="contentinfo"]{
	
	width: 100%;
	@include pie-clearfix;
	padding: 20px 10px;
	
	.widget-container {
		width: 25%;
		float: left;
		font-size: 12px;
		padding: 10px;
		a {
			font-size: 12px;
		}
		
		p {
			margin: 0 0 10px 0;
		}
		
		@media screen and (max-width:768px) {
			width: 100%;
			margin: 0 0 20px 0;
		}
		
	}
	
	@media screen and (max-width:767px) {
		width: 100%;
		padding: 20px;
	}
	
	.wp-plug {
		width: 100%;
		clear: both;
	}
}


/* ============================================= */

/*	Objects						   			 */ 

/* ============================================= */

.size-full { width: 100%; height: auto;}

.alignright { margin: 0 0 20px 20px; }



/* ============================================= */

/*	MOBILE LAYOUT TWEAKS				   	     */ 
/*  anything I didnt cover above.                */
/* ============================================= */


@media screen and (max-width:480px) {

/* Fix google map height */
.single #content .mainbar .gmsc {
	height: 250px !important;
}

/* Homepage digest view */
.home, .page, .attachment {
	#content .mainbar {
		h2 {
			font-size: 9px;
			padding: 0 5px;
			float: left;
			width: 75%;
			font-family: $sans;
		}
		
		footer {
			display: none;
		}
			
		.post .entry-wrapper {
			border-bottom: 1px solid #EEE;
			padding-bottom: 5px;
			padding-top: 5px;
			
			.ui-block-1 {
				width: 20%;
			}
			.post-thumb {
				display: block;
				float: right;
				height: 50px;
				overflow: hidden;
			}
			
			.entry-content, .entry-summary {
				display: none;
			}
		}/* post entry-wrapper */
	}/* mainbar */
}

#page-descrip {
	display: none; // just hide it altogether
	
	.entry-content {
		padding: 5px;
	}
	
	.entry-title {
		font-family: $pine;
		font-size: 1.5em;
		color: $beat-text;
	}
}

