/* ============================================= */
/*	Universal						   			 */ 
/* ============================================= */

a 		{ 	text-decoration: none; color: inherit; }
strong	{	font-weight: bold;	}
em   	{	font-style: italic;	}

body {
	padding: 0 10px;
	font-family: $ff-meta;
	font-size: 15px;

	@media (min-width:1200px){
		padding: 0;
	}
}

h1, .h1, h2, .h2, h3, .h3, h4, .h4, h5, .h5, h6, .h6 {
	font-family: $ff-tisa;

	a {
		color: $text-black;
	}
}

.h1 {
	font-size: 32px;
	color: $txt-black;
}

.h2 {
	font-size: 24px;
	color: $txt-black;
}

.h3, h3 {
	font-size: 18px;
	color: $txt-black;
	font-family: $ff-tisa;
	font-weight: bold;
}

.text {
	font-family: $ff-meta;
	font-size: 15px;
}


/**************************************/

/*			Objects

/**************************************/

.hdr-box {
	padding: 10px;
	// background-color: #f4f4f4;
	background-color: $b-blue;
	border-bottom: 2px solid darken($b-blue, 10%);
	color: darken($b-blue, 40%);
	font-size: 18px;
	text-transform: uppercase;
	font-family: $ff-meta;
	margin-bottom: 10px;
}

/***************************************/

/*              Header

/***************************************/


.menu-header {
	min-height: 60px;

	.menu-logo {
		text-align: center;
		font-size: 40px;
		padding: 10px 0 0 0;
		a {
			background-repeat: no-repeat;
			background-position: top left;
			text-indent: -9000px;
			max-width: 210px;
			height: 75px;
			display: block;
			margin: 0 0 10px 0 ;
			background-image: url("/wp-content/themes/_design/_img/beat-logo-stacked.png");
			@include background-size(100%);

			@media (min-width:321px){
				width: 70%;
				max-width: 300px;
			}

			@media (min-width:599px){
				width: 85%;
				max-width: 600px;
				margin: 0;
				height: 60px;
				background-image: url("/wp-content/themes/_design/_img/beat-logo.png");
			}

			@media (min-width:768px){
				max-width: 600px;
				margin: 0 auto;
				height: 40px;
				background-image: url("/wp-content/themes/_design/_img/beat-logo.png");
			}
		}
	}

	.menu-descrip {
		display: none;
	}

	@media (min-width: 768px){
		.menu-descrip {
			display: block;
			text-align: center;
			font-family: $ff-meta;
			font-size: 15px;
			margin: 10px 0;
		}
	}
}

#menu-trigger {
	display: block;
	position: absolute;
	top: 10px;
	right: 10px;
	height: 60px;
	width: 60px;
	background-color: $b-blue;
	z-index: 10000;
	@include trans;
	@include border-radius(5px);

	.menu-triangle {
		display: block;
		margin: 25px auto 0 auto;
		width: 20px;
		@include triangle(down, 10px, #FFF);

		&.trigger-active {
			@include rotate(180deg);
		}
	}

	@media (min-width: 700px){
		display: none;
	}
}

#access {
	li a {
		display: block;
		padding: 10px;
		text-transform: uppercase;
		font-family: $ff-meta;
	}

	@media (max-width: 699px){
		background-color: white;
		@include box-shadow(0px 0px 10px rgba(0,0,0,.5));
		position: absolute;
		z-index: 9999;
		left: 6%;
		right: 6%;
		top: -500px;
		@include trans;

		&.menu-active {
			top: 20px;
		}

		#searchform {
			width: 100%;
			padding: 10px;
			position: absolute;
			background-color: #FFF;
			// bottom: 0;

			.screen-reader-text {
				display: none;
			}
		}
	}

	@media (min-width: 700px){
		width: 100%;
		max-width: 1200px;
		height: 50px;
		margin: 0 0 20px 0;
		background-color: $b-blue;
		border-bottom: 5px solid darken($b-blue, 20%);

		.menu-header {
			height: 50px;
			width: 80%;
			float: left;

			ul li {
				@include inline-block;

				a {
					font-family: $ff-meta;
					font-weight: bold;
					font-size: 18px;
					color: #FFF;
					padding: 15px 5px;
					height: 50px;
					display: block;
					text-overflow:ellipsis;

					&:hover {
						color: darken($b-blue, 20%);
						border-bottom: 5px solid #000;
					}

					@media (min-width: 768px){
						padding: 15px 10px;
					}
				}

				&.current-menu-item a, &.current-post-parent a{
					color: darken($b-blue, 20%);
					border-bottom: 5px solid #000;
				}

			}
		}

		#searchform {
			@extend .form;
			width: 19%;
			float: right;
			text-align: center;
			padding: 8px;
			// margin: 5px 1% 0 0;
			// background-color: lighten($b-blue, 10%);
			// @include border-radius(8px);
			.screen-reader-text {
				display: none;
			}

			#s {
				width: 50%;
				height: 50px;
			}

			#searchsubmit {
				background-color: darken($b-blue,10%);
				width: 25px;
				height: 25px;
				border: 0;
			}
		}
	}
}// #access

.site-container {
	max-width: 1100px;
	margin: 0 auto;
}

#content {
	@media (min-width:900px) {
		@include ui-grid-lr(70%, 30%);
	}
}

/***************************************/

/*              Sidebar.php

/***************************************/

.xoxo {
	margin-bottom: 30px;
}

.tweets {
	li {
		margin-bottom: 15px;
		font-family: $ff-meta;
		font-size: 15px;

		a {
			color: $link;

			&.timesince {
				color: #CCC;
				font-size: 12px;
				text-transform: uppercase;
			}
		}
	}
}// tweets



/***************************************/

/*              Home.php

/***************************************/

#home-cover {
	margin: 0 0 20px 0;
}

#home-cover-img {
	max-height: 350px;
	overflow: hidden;

	img {
		max-width: 100%;
		height: auto;
	}

	@media (max-width: 480px){
		margin-top: -75px;
		z-index: -10000;
	}
}

#home-cover-txt {
	border-top: 10px solid rgba(255,255,255,.25);
	margin-top: -10px;
	padding: 10px;
	background-color: rgba(255,255,255,.75);
	@include box-shadow(0 0 30px #FFF inset);
	position: relative;
	font-size: 16px;
	font: normal 16px/24px "Georgia", Times, Times New Roman, serif;
	
	p a {
		color: $link;
	}

	.cover-entry-title {
		font-size: 20px;
		line-height: 34px;
		color: $txt-black;
	}

	@media (min-width: 600px) {
		.cover-entry-title {
			font-size: 32px;
			color: $txt-black;
			margin-bottom: 10px;

		}
	}
}

.home-tz {
	margin-bottom: 20px; 
	.entry-title {
		margin-bottom: 10px;
		a {
			@extend .h2;
		}
	}

	p {
		font: normal 14px/20px $ff-meta;
		a {
			color: $link;
		}
	}
}

.home-tz-img {
	img {
		max-width: 100%;
		height: auto;
	}
}
.home-tz-mod {
	padding: 10px;
	@include ui-grid-lr(25%, 75%, 0);
}

.home-zones {
	@media (min-width:900px) {
		width: 70%;
	}
}

.home-zones-cat {
	margin: 0 0 20px 0;

	.home-zones-hdr {
		@extend .h2;
		border-bottom: 3px solid #EEE;
		margin: 0 0 20px 0;
	}

	.home-zones-tiles {
		@media (min-width:500px) {
			@include media-grid(5, 10px);
		}
	}

	.home-zones-title {
		@extend .h3;

		@media (min-width:500px) {
			border-top: 10px solid rgba(255,255,255,.25);
			margin-top: -10px;
			background-color: rgba(255,255,255,.75);
			@include box-shadow(0 0 30px #FFF inset);
			position: relative;
		}
	}

	.home-zones-img {
		display: none;

		@media (min-width:500px) {
			display: block;
			margin-bottom: 10px;
			max-height: 150px;
			overflow: hidden;
			img{
				width: 100%;
				height: auto;
			}
		}
	}

	.home-zones-list {
		padding: 10px 0 0 0;
		li {
			margin: 0 0 10px 0;
		}
		a {
			font-family: $ff-tisa;
			color: #000;
			font-size: 18px;
			font-weight: bold;
		}
	}
}

/***************************************/

/*       Single.php,  Page.php

/***************************************/

.single-content, .page-content {
	h1 {
		@extend .h1;
		border-bottom: 3px solid #EEE;
		margin: 0 0 20px 0;
	}

	.entry-meta {
		margin: 20px 0 20px 0;

		.fb-like {
			float: right;
			display: block;
		}
	}

	.single-related {
		margin: 20px 0 30px 0;

		.related-hdr {
			@extend .hdr-box;
		}

		ul {
			@include media-grid(4,10px);

			li {
				@extend .media-block;

				a {
					color: $text-black;
					@extend .h3;
				}
			}
		}
	}

	.entry-utility {
		border-top: 3px solid #EEE;
		padding: 10px 0;
	}
	#nav-below {
		@include pie-clearfix;
		border-top: 3px solid #EEE;
		border-bottom: 3px solid #EEE;
		margin: 0 0 20px 0;
	}

	.nav-previous {
		float: left;
		width: 50%;
		border-right: 1px solid #EEE;
		padding: 10px;
	}

	.nav-next {
		float: right;
		width: 50%;
		padding: 10px;
	}

	.nav-previous, .nav-next {
		display: block;
		&:hover {
			background-color: #F4F4F4;
		}
	}

	.size-full {
		width: 100%;
		height: auto;
	}

	.alignright {
		float: right;
	}

	.alignleft {
		float: left;
	}


	.h1, .h2, .h3, .h4, .h5, .h6 {
		a {
			color: $text-black;
		}
	}

	a {
		color: $link;
	}

	p {
		margin: 0 0 20px 0;
		font: normal 16px/29px "Georgia", Times, Times New Roman, serif;
		color: $text-black;
	}
}


#comments-title {
	@extend .hdr-box;
}

.respond-header {
	@extend .hdr-box;
}

ol {
	list-style-type: none;
}

.pingback p{
	font-style: italic;
	color: #CCC;
	font-family: $ff-meta;
}

.comment {
	margin-bottom: 20px;
	padding: 0 10%;
	@include pie-clearfix;
	.comment-author {
		border: 1px solid #EEE;
	}

	.comment-meta {
		position: relative;
		float: right;
		top: -30px;
		right: 10px;
	}
}

.entry-content {
	width: 80%;
	margin: 0 auto;
}


/***************************************/

/*       Grid page
// Using this page for the "Seven Nights" feature

/***************************************/

.page-grid {

	.dropdown {
		float: right;
		width: 100%;

		@media (min-width: 600px){
			width: 200px;
			margin-top: -50px;
		}
		.dropdown-trigger-link {
			padding: 10px;
			width: 100%;
			display: block;
			background-color: #FFF;
			border: 1px solid #EEE;
		}
		span {
			display: block;
			margin: 5px 5px 0 0;	
			float: right;
			width: 16px;
			@include triangle(down, 8px, $b-blue);
		}

		.dropdown-menu {
			background-color: #FFF;
			width: 100%;

			@media (min-width: 600px){
				width: 200px;
			}
			left: 0;
			@include box-shadow(0px 0px 10px #CCC);

			li a {
				padding: 10px;
				border-bottom: 1px solid #EEE;
				display: block;

				&:hover {
					background-color: lighten($b-blue, 20%);
				}
			}
		}
	}

	.entry-content{
		width: 100%;

		h3 {
			margin: 0 0 20px 0;
		}
		ul {
			margin: 0 0 20px 0;

			@media (max-width: 599px){
				@include media-grid(2, 20px);
			}

			@media (min-width: 600px){
				@include media-grid(4, 20px);
			}

			li {
				@extend .media-block;
				margin-bottom: 20px;
			}
		}
	}
}


/* ---------------------------------------------- */

/*	Dropdowns						 			  */ 

/* ---------------------------------------------- */

/* !Universal Dropdown Styles */





/***************************************/

/*       Gallery styles
// The way wordpress does this sucks. DL lists. Keep looking for a better way.

/***************************************/

.gallery {
	.gallery-item {
		float: left;
		margin: 0 20px 20px 0;

		&:nth-child(3){
			margin-right: 0;
		}
	}
}


/***************************************/

/*       Category.php

/***************************************/

.category-content, .search-content, .tag-content {

	h1 {
		@extend .h1;
		font-family: $ff-meta;
		border-bottom: 3px solid #EEE;
		margin: 0 0 20px 0;
	}
	#nav-below, #nav-above {
		@include pie-clearfix;
		border-top: 3px solid #EEE;
		border-bottom: 3px solid #EEE;
		margin: 0 0 20px 0;
	}

	.nav-previous {
		float: left;
		width: 50%;
		border-right: 1px solid #EEE;
		padding: 10px;
	}

	.nav-next {
		float: right;
		text-align: right;
		width: 50%;
		padding: 10px;
	}
}

.post {
	@include pie-clearfix;
	margin: 0 0 30px 0;
	p {
		margin: 0 0 20px 0;
		font: normal 16px/24px "Georgia", Times, Times New Roman, serif;
		color: $text-black;
	}

	.entry-title {
		@extend .h2;
	}

	.entry-summary, .entry-meta {
		a {
			color: $link;
		}
	}
	.entry-utility {
		border-top: 3px solid #EEE;
		padding: 10px 0;
		font-size: 14px;
		a {
			color: $link;
		}
	}
	.entry-image {
		float: left;
		margin: 0 20px 20px 0;
	}

	.entry-text {

	}
}


.tag-content {
	h1 {
		@extend .h2;
	}
}

.footer {
	width: 100%;
	background-color: #EEE;
	height: 60px;
}


